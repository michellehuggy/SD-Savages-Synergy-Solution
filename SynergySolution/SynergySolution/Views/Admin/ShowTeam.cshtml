@model IEnumerable<SynergySolution.Models.AgileTeam>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_adminLayout.cshtml";
}


<table class="table" border="1">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.teamName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.teamDescription)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.teamName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.teamDescription)
        </td>
        <td>
            @Html.ActionLink("Edit", "EditTeam", new { id=item.teamId }) |
           
            @Html.ActionLink("Delete", "DeleteTeam", new { id=item.teamId })
        </td>
    </tr>
}

</table>
